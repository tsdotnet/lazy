{
  "name": "@tsdotnet/lazy",
  "version": "1.0.25",
  "description": "Lazy<T> and ResettableLazy<T> class for simplifying lazy initialization.",
  "author": "electricessence",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "dist/index.d.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "umd:main": "dist/umd/index.js",
  "unpkg": "dist/umd/index.js",
  "jsdelivr": "dist/umd/index.js",
  "keywords": [
    "lazy",
    "instantiation",
    "initialization",
    "initializer",
    "lazy init",
    "lazy initialize",
    "lazy initializer",
    "value resolver",
    "value resolution"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:umd": "tsc -p tsconfig.umd.json",
    "build:types": "cpy \"src/**/*.d.ts\" dist --parents && tsc -p tsconfig.types.json",
    "build": "pnpm i && pnpm run clean && concurrently \"pnpm:build:*\"",
    "bump": "pnpm run precommit && pnpm version patch",
    "docs": "rimraf docs && typedoc --options typedoc.json --readme none",
    "lint": "eslint src/**/*.ts",
    "precommit": "pnpm i && pnpm run build && pnpm run lint && pnpm run validate && pnpm run test && git status -s",
    "prepublishOnly": "pnpm run build && pnpm run validate && pnpm run test",
    "preversion": "pnpm run lint && pnpm run test",
    "postversion": "git push && git push --tags && pnpm run docs && git add -A && git commit -m \"Updated docs.\" docs && git push",
    "test": "tsx node_modules/mocha/bin/_mocha tests/**/*.ts",
    "validate": "node ./.build/validate-package.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsdotnet/lazy.git"
  },
  "bugs": {
    "url": "https://github.com/tsdotnet/lazy/issues"
  },
  "homepage": "https://github.com/tsdotnet/lazy#readme",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.3.0",
    "chai": "^5.2.1",
    "concurrently": "^9.2.0",
    "cpy-cli": "^6.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-n": "^17.21.3",
    "mocha": "^11.7.1",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.4",
    "typedoc": "^0.28.10",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1"
  },
  "dependencies": {
    "@tsdotnet/disposable": "^1.3.1",
    "tslib": "^2.8.1"
  },
  "pnpm": {
    "overrides": {
      "hosted-git-info@<2.8.9": ">=2.8.9",
      "y18n@=4.0.0": ">=4.0.1",
      "lodash@<4.17.20": ">=4.17.20",
      "shell-quote@<=1.7.2": ">=1.7.3",
      "lodash@<4.17.21": ">=4.17.21",
      "json5@<1.0.2": ">=1.0.2",
      "json5@>=2.0.0 <2.2.2": ">=2.2.2",
      "minimatch@<3.0.5": ">=3.0.5",
      "semver@<5.7.2": ">=5.7.2",
      "semver@>=6.0.0 <6.3.1": ">=6.3.1",
      "semver@>=7.0.0 <7.5.2": ">=7.5.2",
      "get-func-name@<2.0.1": ">=2.0.1",
      "@babel/traverse@<7.23.2": ">=7.23.2",
      "braces@<3.0.3": ">=3.0.3"
    }
  }
}
